		<script>
						$.getScript('https://web.archive.org/web/20220514155115/https://players.yumpu.com/libs/yegsi-min.js');

			function onCanvasSizeChange() {
				var noTitleSubtractOffset = ( '' != '' ) ? 60 : 0;
				var eWH;
				if ( ($('#embedTitleField').height() != null && $('#embedTitleField').css('display') != 'none') || ($('#embedUsernameField').height() != null && $('#embedUsernameField').css('display') != 'none' ) ) {

					eWH = $(window).height() - 60; // 60 == sum(margins top bottom)
					eWH = ( $('#embedTitleField').css('display') != 'none' ) ? eWH - $('#embedTitleField').height() : eWH;
					eWH = ( $('#embedUsernameField').css('display') != 'none' ) ? eWH - $('#embedUsernameField').height() : eWH;
                    if(player != "hornet" && player != "hornetmobile") {
                        $('#embedWrapper').css('height', eWH.toString() + 'px');
                        $('#embedWrapper').attr('height', eWH.toString());
                        $('#embedWrapper').height(eWH);	// set flip-embed to rest of height (regarding title and username height)
                    } else {
                        $('#embedWrapper').css('height', '100%');
                    }
					eWH = (eWH - noTitleSubtractOffset);

					if ( player == "html5mobile" ) {
						$('#embedWrapper').height( eWH );	// set flip-embed to rest of height (regarding title and username height)
					}

				} else {

										var hTitH = ( $('#embedTitleField').height() != null && $('#embedTitleField').css('display') != 'none' ) ? $('#embedTitleField').height() : 0;
					var hUNH = ( $('#embedUsernameField').height() != null && $('#embedUsernameField').css('display') != 'none' ) ? $('#embedTitleField').height() : 0;
					eWH = $(window).height() - hTitH - hUNH - 60;
                    if(player != "hornet" && player != "hornetmobile") {
                        $('#embedWrapper').css('height', eWH + 'px');	// set flip-embed to rest of height (regarding title and username height)
                    } else {
                        $('#embedWrapper').css('height', '100%');
                    }
					if ( player == "html5mobile" ) {
						$('#embedWrapper').height( $(window).height() - hTitH - hUNH );	// set flip-embed to rest of height (regarding title and username height)
					}

				}

				if(player = "eagle"){
                    $('#embedWrapper').css('position', 'absolute');
                    $('#embedWrapper').css('width', '100%');
                    $('#embedWrapper').css('height', '100%');
                    $('#embedWrapper').css('top', '0');
                }
			}

			$(document).ready(function(){
				onCanvasSizeChange();
				$(window).resize(function() {
					documentHeightResize(true);
				});

			});
		</script>

