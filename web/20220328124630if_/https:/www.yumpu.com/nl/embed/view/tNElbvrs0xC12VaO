						var iFrameCommObj;
			function return_current_page_number( cp ) {
				try {
					iFrameCommObj.source.postMessage( cp.toString(), iFrameCommObj.origin );
				} catch ( err ) {}
			}
			try {					window.addEventListener('message', function (m) {
					try {
						var message = m.data;
						switch (message) {
							case "yumpuPlayer_flipForward":
								yegsi_interface("flipForward");
								break;
							case "yumpuPlayer_flipBackward":
								yegsi_interface("flipBackward");
								break;
							case "yumpuPlayer_getCurrentPage":
								var cp = yegsi_interface("getCurrentPage");
								if (player != "flash") {        									if (typeof(cp) != 'undefined') {
										m.source.postMessage(cp.toString(), m.origin);
									} else {
										m.source.postMessage('0', m.origin);
									}
								}
								iFrameCommObj = m;
															break;
						}
					} catch (err) {
					}
				});
			} catch(err) {}
