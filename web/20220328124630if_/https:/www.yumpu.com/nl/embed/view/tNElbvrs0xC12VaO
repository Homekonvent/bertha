    <script>
        var yp_ad_loaded = false;
        var yp_ad_loaded_page_4 = false;
        var yp_ad_last_page_loaded = false;
        var yp_document_pages = 29

        setInterval(function() {
            var yp_ad_current_page = 0;

            try{
                if(typeof window['yumpu_hornet_0'] != "undefined") {

                    yp_ad_current_page = window["yumpu_hornet_0"].services.get_current_index();

                } else if(typeof window['yumpu_eagle_api_0'] != "undefined") {

                    yp_ad_current_page = window["yumpu_eagle_api_0"].mainRef.currentPage;

                }

            } catch(err){}


            if(yp_ad_current_page > 0 && !yp_ad_loaded) {
                loadBanner();
                yp_ad_loaded = true;
            }

            if(yp_ad_current_page == 4 && !yp_ad_loaded_page_4) {
                loadBanner();
                yp_ad_loaded_page_4 = true;
            }

            if(yp_document_pages >= 5 && !yp_ad_last_page_loaded && yp_document_pages <= (yp_ad_current_page + 1)) {
                loadBanner();
                yp_ad_last_page_loaded = true;
            }


        }, 3000);
    </script>
