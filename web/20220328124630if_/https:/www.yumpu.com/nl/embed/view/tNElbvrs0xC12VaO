    <script>
        const resizeObserver = new ResizeObserver(entries => yp_ad_element_ready(entries[0].target));

        resizeObserver.observe(document.getElementById('yp-yieldlove-layer-container'));

        function yp_ad_element_ready(e) {
            if(document.body.clientWidth < 2000 || document.body.clientHeight < e.clientHeight)
                return e.style.display = 'none';
            e.style.marginTop = ((document.body.clientHeight - e.clientHeight) / 2) + 'px';
        }
    </script>
